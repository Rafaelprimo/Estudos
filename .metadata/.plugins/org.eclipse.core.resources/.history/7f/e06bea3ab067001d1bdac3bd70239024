package negativos;

public class principal {

	public static void main(String[] args) {

		System.out.println("Digite um número entre 0 e 10");
		int n = sc.nextInt();
		System.out.println();
		
		while (n<0 || n>10) {
			System.out.println("Digite um número entre 0 e 10");
			n = sc.nextInt();
		}
		System.out.println();
		int[] vect = new int[n];
		
		for (int i=0; i<n; i++) {
			System.out.println();
			System.out.println("Digite um número");
			vect[i] = sc.nextInt();
			}
		for (int i=0; i<n; i++) {		
			if (vect[i]<0) {
				System.out.println(vect[i]);
				}
			}
		}
	}